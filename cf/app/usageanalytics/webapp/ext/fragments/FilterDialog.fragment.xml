<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<ViewSettingsDialog id="filterDialogFragmentViewSettingsDialog1"
		confirm="handleFilterDialogConfirm">
		<filterItems>
			<ViewSettingsFilterItem id="filterDialogFragmentViewSettingsFilterItem1"
				text="Cost"
				key="cost"
				multiSelect="false">
				<items>
					<ViewSettingsItem id="filterDialogFragmentViewSettingsItem1" text="not 0" key="cost___GT___0___X" />
				</items>
			</ViewSettingsFilterItem>
			<ViewSettingsFilterItem id="filterDialogFragmentViewSettingsFilterItem2"
				text="Price"
				key="Price"
				multiSelect="false">
				<items>
					<ViewSettingsItem id="filterDialogFragmentViewSettingsItem4" text="Less Than 100" key="Price___LE___100___X" />
					<ViewSettingsItem id="filterDialogFragmentViewSettingsItem5" text="Between 100 and 1000" key="Price___BT___100___1000" />
					<ViewSettingsItem id="filterDialogFragmentViewSettingsItem6" text="More Than 1000" key="Price___GT___1000___X" />
				</items>
			</ViewSettingsFilterItem>
			<ViewSettingsFilterItem id="filterDialogFragmentViewSettingsFilterItem3"
				text="Subaccount"
				key="subaccountName"
				multiSelect="false">
				<items>
					<ViewSettingsItem id="filterDialogFragmentViewSettingsItem7" text="Contains" key="subaccountName___Contains___IS03 - Integration (PRD)___X" />
					<ViewSettingsItem id="filterDialogFragmentViewSettingsItem8" text="Between 1000 and 2000" key="WeightMeasure___BT___1000___2000" />
					<ViewSettingsItem id="filterDialogFragmentViewSettingsItem9" text="More Than 2000" key="WeightMeasure___GT___2000___X" />
				</items>
			</ViewSettingsFilterItem>
